<h1>Listing users</h1>

<table>
  <tbody>
    <% @users.each do |user| %>
      <tr>
      <td><%= link_to image_tag('edit26.png'), edit_user_path(user) %></td>
        <td><%= link_to image_tag('recycle43.png'), user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to user.name, user %></td>
        <td><%= user.email %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
